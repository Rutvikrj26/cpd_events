# =============================================================================
# FRONTEND DEVELOPMENT ENVIRONMENT
# =============================================================================
# Copy this file to .env.dev and fill in your values:
#   cp .env.dev.template .env.dev
#
# Variables must be prefixed with VITE_ to be accessible in the app.
# =============================================================================

# Backend API URL
VITE_API_URL=http://localhost:8000/api/v1

# Google Maps API Key (get from console.cloud.google.com)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Stripe Publishable Key (get from dashboard.stripe.com/apikeys)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_xxx
